package accessnyc.rules;
import accessnyc.response.EligibleProgram

import accessnyc.request.Person
import accessnyc.request.Household
import accessnyc.rules.income.IncomeAdultsTotalMonthly
import accessnyc.rules.expense.ExpenseHouseholdHasHeating


rule "s2_r033"
    dialect "mvel"
    
    when 
        Household(city == "NYC")
        not (Household(livingRentalType == "NYCHA") and not (ExpenseHouseholdHasHeating(value == true)))
        not Household(livingHotel == true)
        not Household(livingShelter == true)
        (
            (Household(members == 1) and IncomeAdultsTotalMonthly(amount <= 2610))
            or (Household(members == 2) and IncomeAdultsTotalMonthly(amount <= 3413))
            or (Household(members == 3) and IncomeAdultsTotalMonthly(amount <= 4216))
            or (Household(members == 4) and IncomeAdultsTotalMonthly(amount <= 5019))
            or (Household(members == 5) and IncomeAdultsTotalMonthly(amount <= 5822))
            or (Household(members == 6) and IncomeAdultsTotalMonthly(amount <= 6625))
            or (Household(members == 7) and IncomeAdultsTotalMonthly(amount <= 6775))
            or (Household(members == 8) and IncomeAdultsTotalMonthly(amount <= 6926))
            or (Household(members == 9) and IncomeAdultsTotalMonthly(amount <= 7486))
            or (Household(members == 10) and IncomeAdultsTotalMonthly(amount <= 8046))
            or (Household(members == 11) and IncomeAdultsTotalMonthly(amount <= 8606))    
        )

    then
        $programcode = new EligibleProgram()
        $programcode.setCode("S2R033")
        insertLogical($programcode)

end